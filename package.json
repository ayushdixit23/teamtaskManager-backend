{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "migrate:create": "node-pg-migrate create -j sql -m ./src/migrations",
    "migrate": "node-pg-migrate up -j sql -m ./src/migrations",
    "migrate:down": "node-pg-migrate down -j sql -m ./src/migrations"
  },
  "keywords": [],
